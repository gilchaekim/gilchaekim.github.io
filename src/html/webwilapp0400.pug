extends ../components/templates/base.pug

block prepend top 
  - mainClassName="webwil flex_top_bot"
  - pageTitle = "대출철회신청"

block content
  +content_group(0)
    +result({
      type:'result',
      mainText:"대출철회 신청서<br>작성이 완료되었습니다.",
      subText:"관리영업점에서 확인 후 고객님께<br>별도로 통지하여 드립니다.",
      img:"/src/images/ico_finish.svg",
    })

  +text({
    type:'warning',
    text:"최초 승인여부는 금리인하요구 조회/취소 메뉴에서 확인 가능합니다.",
    className:"flex_bot"
  })
  +button_bottom_fixed()
    +button({
      label:"확인",
      size:"large",
    })
