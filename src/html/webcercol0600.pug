extends ../components/templates/base.pug

block prepend top 
  - mainClassName="webcer"
  - pageTitle = "제증명 신청/발급"

block content
  +content_group(0)
    +result({
      type:'result',
      mainText:"금융소득종합과세 발급이<br>완료되었습니다.",
      img:"/src/images/ico_example2.png",
    })
  +content_group(0)
  +result({
    dataType:"line",
    data:[
      {
        title:"신청증명서",
        text:"금융거래확인서",
      },
      {
        title:"발급기준일",
        text:"2023.04.04",
