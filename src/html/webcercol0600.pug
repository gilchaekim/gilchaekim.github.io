  +text({
    type:'warning',
    text:"고객님께서 신청하신 제증명이 이메일로 발송되었습니다."
  })

  .btn_wrap
    button(type="button")
      span 상세내용보기
    button(type="button")
      em PDF 다운로드

  +button_bottom_fixed()
    +button({
      label:"확인",
      size:"large",
    })
  
