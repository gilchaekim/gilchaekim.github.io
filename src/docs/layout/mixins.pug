mixin pagesNavLists(params={})
  - const { element = 'div', lists, ...rest } = Object.assign({}, params);

  #{element}(
    class="nav_lists"
  )
    each item, key in lists
      - const name = item[0]
      - const url = item[0]
      - const isTitle = !!!Array.isArray(item);
      div(
        class={"tree-title":isTitle}
        class={"lists":!isTitle}
      )
        span
          button(
            type="button"
            data-href=url
            class={"title":isTitle}
          ) #{key}
          if !isTitle
            a(
              href=url
              target="_blank"
              class="blank"
            ) 새창 열기

      if isTitle
        +pagesNavLists({
          lists:item
        })

mixin guideTitle(text)
  - const { element = 'div', ...rest } = attributes
  #{element}(
    class="title_sec"
  )
    p.title #{text}


mixin guideExample()
  - const { element = 'div', code,  ...rest } = attributes
  #{element}(
    class="guide_card_box"
  )
    block
    if code
      div.ddd
        button code view
    