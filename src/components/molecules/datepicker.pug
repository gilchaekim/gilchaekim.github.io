mixin datepicker(params={})
  //- 공통 요소 Start
  - const prefix = "mui"
  - const divide = '_'
  //- 공통 요소 End
  - const {element='div', className, placeholder, value, quickBtn, ...rest} = Object.assign({}, params);
  if quickBtn
    .inner_quick_btrn
      +radioCheck({
        name:'btnType',
        type:"radio",
        className:'button_type',
        lists:[
          {
            label:'일주일'
          },
          {
            label:'1개월'
          },
          {
            label:'3개월'
          },
          {
            label:'6개월'
          },
          {
            label:'직접입력'
          }
        ]
      })
      #{element}(
        class=`${prefix}${divide}datepicker`
        class=className
        mui-datepicker=""
      )
        +input({
          value:value
        })
  else 
    #{element}(
        class=`${prefix}${divide}datepicker`
        class=className
        mui-datepicker=""
      )
        +input({
          value:value
        })
