mixin button_link(params={})
  //- 공통 요소 Start
  - const prefix = "mui"
  - const divide = '_'
  //- 공통 요소 End
  
  - const { element="div",  className, list = [], ...rest } = Object.assign({}, params)
  #{element}(
    class=`${prefix}${divide}link_text`
    class=className
  )
    each val in list
      a(href="")
        span!=val
mixin button_bottom_fixed(params={})
  //- 공통 요소 Start
  - const prefix = "mui"
  - const divide = '_'
  //- 공통 요소 End
  div(
    class=`${prefix}${divide}button_fixed`
  )
    span.inset
      block