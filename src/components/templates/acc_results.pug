mixin acc_results(type)
  //- 공통 요소 Start
  - const prefix = "mui"
  - const divide = '_'
  - const element = 'div'
  if type == "acc_result"
    #{element}(
      class=`${prefix}${divide}accresult`
    )
      .acc_result
        p.date 02.15
        .lists 
          .text 
            p.txt1 GS 편의점
            p.txt2.out -5,000원
          .time 
            p.txt1 11:08
            p.txt2 3,064,000원

        .lists 
          .text 
            p.txt1 급여
            p.txt2.in 30,000,000원
          .time 
            p.txt1 11:08
            p.txt2 3,064,000원
            
        .lists 
          .text 
            p.txt1 ABC 카페
            p.txt2.out -30,000원
          .time 
            p.txt1 11:08
            p.txt2 3,064,000원

  if type == "acc_result2"
    #{element}(
      class=`${prefix}${divide}accresult`
    )
      .acc_result
        p.date 02.15
        .lists 
          .text 
            p.txt1 7회차
            p.txt2.out -5,000원
          .time 
            p.txt1 11:08
            p.txt2 3,064,000원
