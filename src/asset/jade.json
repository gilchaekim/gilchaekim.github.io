{
	// Place your snippets for jade here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"accordion": {
		"prefix": "!Accordion",
		"body": [
				"+accordion({",
				"\tactive:0",
				"})",
				"\t+acc_title('$1')",
				"\t+acc_content()",
				"\t\t$0"
		],
		"description": "이코디언 리스트"
	},
	"content_group": {
		"prefix": "!content_group",
		"body": [
				"+content_group(${1|1,2,3|})",
				"\t$0"
		],
		"description": "컨텐츠 그룹"
	},
	"Button": {
		"prefix": "!button",
		"body": [
				"+button({",
				"\tlabel:\"$0\",",
				"\tsize:\"${1|small,medium,large|}\",",
				"})"
		],
		"description": "기본 버튼"
	},
	"Button_group": {
		"prefix": "!Button_group",
		"body": [
				"+button_group({",
				"\tbtns:[",
				"\t\t'$0',",
				"\t]",
				"})"
		],
		"description": "버튼 그룹"
	},
	"Button_wrap": {
		"prefix": "!button_wrap",
		"body": [
				"+button_wrap()",
				"\t$0"
		],
		"description": "기본 버튼"
	},
	"dataTable": {
		"prefix": "!dataTable",
		"body": [
				"+dataTable({",
				"\tcaption:\"${1:제목}\",",
				"\tcolgroup:[$0, , ],",
				"\theadData:[",
				"\t\t[",
				"\t\t\t{",
				"\t\t\t\ttext: \"\",",
				"\t\t\t},",
				"\t\t],",
				"\t],",
				"\tbodyData:[",
				"\t\t[",
				"\t\t\t{",
				"\t\t\t\ttext: \"\"",
				"\t\t\t}",
				"\t\t],",
				"\t]",
				"})"
		],
		"description": "데이터 테이블"
	},
	"datepicker": {
		"prefix": "!datepicker",
		"body": [
				"+datepicker({",
				"\t${1:quickBtn:true}",
				"\t${2:value:\"\"}",
				"})"
		],
		"description": "날짜 입력 폼"
	},
	"input": {
		"prefix": "!input",
		"body": [
				"+input({",
				"\t${1:value:\"\",}",
				"\t${2:subfix:\"\",}",
				"})"
		],
		"description": "1"
	},
	"input_money": {
		"prefix": "!input_money",
		"body": [
				"+input_money()"
		],
		"description": "금액 입력 폼"
	},
	"input_money": {
			"prefix": "input_btn",
			"body": [
				"+input_button({",
				"\tbtnText:\"\",",
				"\tplaceholder:\"\",",
				"\ttitle:\"\"",
				"})"
			],
			"description": "input + button"
	},
	"modal": {
		"prefix": "!modal",
		"body": [
				"+modal({",
				"\t${1:title:\"\"}",
				"})",
				"\t$0",
				"\t+footer()"
		],
		"description": "모달 공통"
	},
	"radio&check": {
		"prefix": ["!radio&check", "!ㄱㅁㅇ"],
		"body": [
				"+radioCheck({",
				"\t${1:type:\"radio\",}",
				"\tname:'chk$RANDOM'",
				"})"
		],
		"description": "라디오 & 체크"
	},
	"radio&check_btn": {
		"prefix": ["!radio&check_btn", "!ㄱㅁㅇ_ㅠ수"],
		"body": [
				"+radioCheck({",
				"\tname:'btnType',",
				"\ttype:\"radio\",",
				"\tclassName:'button_type',",
				"\tlists:[",
				"\t\t{",
				"\t\t\tlabel:''",
				"\t\t}",
				"\t]",
				"})"
		],
		"description": ""
	},
	"result": {
			"prefix": ["!result", "!ㄱㄷㄴ"],
			"body": [
					"+result({",
					"\tdata:[",
					"\t\t{",
					"\t\t\ttitle:\"\",",
					"\t\t\ttext:\"\",",
					"\t\t},",
					"\t]",
					"})"
			],
			"description": "결과 리스트"
	},
	"tab": {
		"prefix": ["!tab", "!ㅅ뮤"],
		"body": [
				"+tabsWrap({",
				"\tactive:0,",
				"\tclassName:\"\"",
				"})",
				"\t+tab(\"$0\")"
		],
		"description": "텝"
	},
	"tag": {
		"prefix": ["!tag", "!ㅅㅁㅎ"],
		"body": [
				"+tag({",
				"\ttext:\"\", ",
				"\ttype:\"\"",
				"})",
				""
		],
		"description": "태그"
	},
	"textLists": {
		"prefix": ["!text_lists", "ㅅㄷㅌㅅ"],
		"body": [
				"+textLists({",
				"\ttype:'${1|dot,dot,dot|}',",
				"\tlists:[",
				"\t\t{",
				"\t\t\ttext:\"$0\",",
				"\t\t\tclassName:\"custom_class_name\",",
				"\t\t\t${2:subLists:\"\",}",
				"\t\t},",
				"\t]",
				"})"
		],
		"description": "텍스트 리스트"
	},
	"textarea": {
		"prefix": ["textarea", "ㅅㄷㅌㅅㅁㄱㄷㅁ"],
		"body": [
				"+textarea()"
		],
		"description": "textarea"
	},
	"title": {
		"prefix": ["!title", "!샤ㅅ"],
		"body": [
				"+title({",
				"\ttext:\"$0\",",
				"\tclsLv:${1|1,2,3,4,5,6|},",
				"\ttagLv:${2|1,2,3,4,5,6|},",
				"})"
		],
		"description": "타이틀"
	},
	"collapse": {
		"prefix": "!collapse",
		"body": [
				"+collapse({",
				"\tisOpen:true",
				"})",
				"\t+header()",
				"\t\t",
				"\t+content()",
				"\t\t"
		],
		"description": "collapse"
	},
	"guide_text": {
		"prefix": "!guide_text",
		"body": [
				"+collapse({",
				"\tclassName:\"guide_text\",",
				"\tisOpen:true,",
				"})",
				"\t+header()",
				"\t p.title 알아두세요",
				"\t+content()",
				"\t\t+textLists({",
				"\t\t\ttype:'dot',",
				"\t\t\tlists:[",
				"\t\t\t\t{",
				"\t\t\t\t\ttext:\"\",",
				"\t\t\t\t},",
				"\t\t\t]",
				"\t\t})"
		],
		"description": "알아두세요"
	},
	"form": {
		"prefix": "!form",
		"body": [
				"+form()"
		],
		"description": "form"
	},
	"form_body_value": {
		"prefix": "!form_body_value",
		"body": [
				"+form_body({title:\"$1\"})",
				"\t+form_value({",
				"\t\tvalue:\"$0\"",
				"\t})"
		],
		"description": "텍스트"
	},
	"form_body_select": {
		"prefix": "!form_body_select",
		"body": [
				"+form_body({title:\"$1\"})",
				"+button({",
				"\tlabel:\"$0\",",
				"\tclassName:'select_type'",
				"})"
		],
		"description": "셀렉트"
	},
	"form_body_datepicker": {
		"prefix": "!form_body_datepicker",
		"body": [
				"+form_body({title:\"$1\"})",
				"\t+datepicker()"
		],
		"description": "데이트피커"
	},
	"form_body_datepicker_quick": {
		"prefix": "!form_body_datepicker_quick",
		"body": [
				"+form_body({title:\"$1\"})",
				"+datepicker({",
				"\tquickBtn:true",
				"})"
		],
		"description": "데이트피커 조회기간 버튼"
	},
	"form_body_button_check": {
			"prefix": "!form_body_button_check",
			"body": [
					"\t+form_body({title:\"$1\"})",
					"\t\t+radioCheck({",
					"\t\t\tname:'btnType',",
					"\t\t\t${2:type:\"radio\",}",
					"\t\t\tclassName:'button_type',",
					"\t\t\tlists:[",
					"\t\t\t\t{",
					"\t\t\t\t\tlabel:'$3',",
					"\t\t\t\t\tchecked:true",
					"\t\t\t\t},",
					"\t\t\t\t{",
					"\t\t\t\t\tlabel:''",
					"\t\t\t\t}",
					"\t\t\t]",
					"\t\t})"
			],
			"description": "버튼형 체크 박스"
	}
}

    
